  "description": "低代码平台引擎解析器, 用于解析组件面板属性",
  "main": "index.tsx",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/poohlaha/lower-engine-parser.git"
  },
  "keywords": [
    "React",
    "Lower",
    "Engine",
    "Parser"
  ],
  "author": "poohlaha",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/poohlaha/lower-engine-parser/issues"
  },
  "homepage": "https://github.com/poohlaha/lower-engine-parser#readme",
  "scripts": {
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "antd-mobile": "^5.38.1",
    "crypto-js": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.3.2",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "dayjs": "^1.11.13"
  },
  "devDependencies": {
    "core-js": "3",
    "husky": "^8.0.3",
    "prettier": "^2.4.1",
    "lint-staged": "^13.0.3",
    "babel-plugin-import": "^1.13.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-jsx-control-statements": "^2.2.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8",
    "iOS >= 8",
    "Firefox >= 20",
    "Android > 4.4"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.{vue,jsx,js,tsx,ts,json,css,md}": [
      "prettier --write"
    ],
    "**/*.{vue,jsx,js,tsx,ts}": [
      "eslint --fix"
    ]
  }
